             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LaiLaiBear"
             StartupUri="StartUp.xaml">
    <Application.Resources>
        <ContextMenu x:Key="ParasiteMenu">
            <MenuItem Name="ParasiteDelete" Header="删除" Click="ParasiteDelete_Click"/>
        </ContextMenu>

        <ContextMenu x:Key="UserMenu">
